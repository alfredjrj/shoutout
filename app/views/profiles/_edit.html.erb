
<h2 id="socialmedia">  social media accounts </h2>
 <%= @user.profile.id %>
  <%= form_for @user.profile  do |f| %>
 <div class="field">
    <%= f.label :instagram %><br />
    <%= f.text_field :instagram, autocomplete: "off"  , class:"form-control" %>
  </div>
   <div class="field">
    <%= f.label :twitter %><br />
    <%= f.text_field :twitter, autocomplete: "off"  , class:"form-control" %>
  </div>
   <div class="field">
    <%= f.label :facebook %><br />
    <%= f.text_field :facebook, autocomplete: "off"  , class:"form-control" %>
  </div>
   <div class="field">
    <%= f.label :youtube %><br />
    <%= f.text_field :youtube, autocomplete: "off"  , class:"form-control" %>
  </div>
  



<h2 id="general">  General  </h2>

<div class="field">
    <%= f.label :bio %><br />
    <%= f.text_area:bio, autocomplete: "off"  , class:"form-control" %>
</div>

<div class="field">
    <%= f.label :interests %><br />
    <%= f.text_field :interests, autocomplete: "off"  , class:"form-control" %>
</div>

<div class="actions">
    <%= f.submit "Update" , class:"btn btn-primary btn-md" %>
</div><br> 
<% end %> 
