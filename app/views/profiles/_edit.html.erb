


  <%= form_for @user.profile  do |f| %>
 
<h2 id="general">  General  </h2>

<div class="field">
    <%= f.label :bio %><br />
    <%= f.text_area:bio, autocomplete: "off"  , class:"form-control" %>
</div>

<div class="field">
    <%= f.label :interests %><br />
    <%= f.text_field :interests, autocomplete: "off"  , class:"form-control" %>
</div>

<div class="actions">
    <%= f.submit "Update" , class:"btn btn-primary btn-md" %>
</div><br> 
<% end %> 